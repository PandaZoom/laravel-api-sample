    "description": "Laravel user locale functionality.",
    "keywords": [
        "Laravel",
        "passport",
        "register"
    ],
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Vlad Radchenko",
            "email": "westen7ua@gmail.com"
        }
    ],
    "require": {
